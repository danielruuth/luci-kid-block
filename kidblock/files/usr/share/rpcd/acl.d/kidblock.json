{
  "luci-app-kidblock": {
    "description": "Grant access to kidblock UCI and ubus",
    "read": {
      "uci": [ "kidblock" ],
      "ubus": {
        "kidblock": [ "status" ]
      }
    },
    "write": {
      "uci": [ "kidblock" ],
      "ubus": {
        "kidblock": [ "apply", "override", "clear_override" ]
      }
    }
  }
}
